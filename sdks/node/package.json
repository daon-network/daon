{
  "name": "daon-sdk",
  "version": "1.0.0",
  "description": "DAON Creator Protection SDK for Node.js applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:types": "tsc --project tsconfig.types.json",
    "test": "jest",
    "prepare": "npm run build",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "keywords": [
    "creator-protection",
    "blockchain",
    "copyright",
    "content",
    "fanfiction",
    "ai-protection"
  ],
  "author": "DAON Network",
  "license": "SEE LICENSE IN ../../LICENSE.md",
  "homepage": "https://github.com/daon-network/node-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/daon-network/node-sdk.git"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
